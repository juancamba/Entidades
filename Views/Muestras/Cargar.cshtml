@{
    ViewData["Title"] = "Cargar conjunto muestras";
}


<h2 class="text-primary">Cargar conjunto muestras</h2>

<div class="container mt-3">
    <div class="row justify-content-center">
        <div class="col-md-4">
            <form method="post" asp-action="Cargar" enctype="multipart/form-data">
            
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group mt-3">
                    <label for="subidaArchivo">Archivo muestras</label>
                     <input type="file" name="file" id="subidaArchivo" class="form-control" />
                    <button type="submit" class="btn btn-primary form-control">Crear</button>
                </div>

            </form>
        </div>
    </div>
</div>
<div class="container ">
    <div class="row justify-content-center mt-3">
        <div class="col-md-4">
             <partial name="_VistaCrearVolver" />
        </div>
    </div>
</div>